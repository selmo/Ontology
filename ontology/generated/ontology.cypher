CREATE (:Term {id: 'T04030002', name_ko: '저소득층', name_en: 'Low-Income Groups', acronym: 'None', description: '빈곤층, 기초생활수급자 등 경제적으로 어려운 계층. 기초생활보장제도, 긴급복지지원 등의 대상.', display_name: '저소득층 (:Term)'});
